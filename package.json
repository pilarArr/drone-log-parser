{
  "name": "drone-log-parser",
  "version": "2.1.1",
  "description": "A package to parse drone log files to consumable json files",
  "main": "build/index.js",
  "dependencies": {
    "line-reader": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^22.0.6"
  },
  "scripts": {
    "build": "babel src -d build --ignore '**/*.test.js'",
    "dev": "babel src -d build --ignore '**/*.test.js' --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "release": "np"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "node": true
    },
    "globals": {
      "expect": false
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "no-tabs": [
        "error"
      ],
      "no-mixed-spaces-and-tabs": [
        "error"
      ],
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "no-underscore-dangle": [
        "error"
      ],
      "class-methods-use-this": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pilarArr/drone-log-parser.git"
  },
  "keywords": [
    "npm",
    "log",
    "drone",
    "rpa",
    "parser",
    "pixhawk",
    "apm",
    "ardupilot"
  ],
  "author": "pilarArr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pilarArr/drone-log-parser/issues"
  },
  "homepage": "https://github.com/pilarArr/drone-log-parser#readme"
}
